{
  "name": "interfacer-gui",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix-lint": "eslint . --ext ts --ext tsx --ext js --fix",
    "format": "prettier --write .",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "i18n:collect": "i18next -c i18n-parser.config.mjs '{pages,components}/**/*.{js,ts,jsx,tsx}'",
    "reload": "pm2 startOrGracefulReload ecosystem.config.js",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --config videoRecording=false",
    "test:component": "cypress run-ct --record",
    "screenshots": "cypress run --spec 'cypress/e2e/screenshots/*.cy.ts' --config baseUrl='https://interfacer-gui-staging.dyne.org/'",
    "e2e": "pnpm build && start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "pnpm build && start-server-and-test start http://localhost:3000 cypress:headless",
    "test": "pnpm build && start-server-and-test start http://localhost:3000 cypress:headless",
    "translate": "./bin/translate.sh",
    "prepare": "husky install",
    "release": "HUSKY=0 standard-version --no-verify",
    "types:generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@babel/core": "^7.19.3",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.10",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/cypress": "^8.0.3",
    "@types/base45": "^2.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/parser": "^5.40.1",
    "@uiw/react-markdown-preview": "^4.1.0",
    "apollo-client": "^2.6.10",
    "apollo-utilities": "^1.3.4",
    "autoprefixer": "^10.4.8",
    "base45": "^2.0.1",
    "boring-avatars": "^1.7.0",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "cypress": "^10.7.0",
    "cypress-localstorage-commands": "^2.2.1",
    "daisyui": "^2.25.0",
    "dayjs": "^1.11.5",
    "graphql": "^15.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "next": "12.2.5",
    "next-i18next": "^12.0.1",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.38.0",
    "react-markdown-editor-lite": "^1.3.3",
    "react-qr-code": "^2.0.7",
    "react-select": "^5.4.0",
    "sass": "^1.54.8",
    "swiper": "^8.4.4",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "yup": "^0.32.11",
    "zenroom": "2.2.3-f0193d51"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "cypress": "^10.7.0",
    "cypress-fail-fast": "^5.0.1",
    "devmoji": "^2.3.0",
    "eslint": "^8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-i18next": "6.0.0-2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "i18next-parser": "^6.6.0",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0"
  }
}
